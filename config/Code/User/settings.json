  "launch": {
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Chrome tizen simulator",
        "file": "D:/tizen-studio/tools/websimulator/web/ripple.html?url=file:///${workspaceFolder}/index.html&platform=wearable-2.0",
        "webRoot": "${workspaceFolder}",
        "userDataDir": true,
        "runtimeArgs": [
          "--no-first-run",
          "--activate-on-launch",
          "--no-default-browser-check",
          "--allow-file-access-from-files",
          "--disable-translate",
          "--disable-web-security",
          "--user-data-dir=D:/tizen-studio/tools/websimulator/web/",
          "--proxy-auto-detect",
          "--proxy-bypass-list=127.0.0.1"
        ]
      },
      {
        "name": "Launch chrome",
        "type": "chrome",
        "request": "launch",
        "port": 4100,
        "url": "http://localhost:4220/",
        "webRoot": "${workspaceFolder}",
        "runtimeArgs": [
          "--disable-web-security",
          "--disable-features=CrossSiteDocumentBlockingAlways,CrossSiteDocumentBlockingIfIsolating"
        ]
      },
      {
        "name": "Launch firefox",
        "type": "firefox",
        "request": "launch",
        "url": "http://localhost:4220/",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Launch edge",
        "type": "edge",
        "request": "launch",
        "url": "http://localhost:4220/",
        "webRoot": "${workspaceFolder}",
        "runtimeArgs": [
          "--disable-web-security --disable-features=CrossSiteDocumentBlockingAlways,CrossSiteDocumentBlockingIfIsolating"
        ]
      },
      {
        "name": "Launch karma",
        "type": "chrome",
        "request": "launch",
        "port": 9333,
        "url": "http://localhost:9877/debug.html",
        "webRoot": "${workspaceFolder}",
        "runtimeArgs": [
          "--disable-web-security --disable-features=CrossSiteDocumentBlockingAlways,CrossSiteDocumentBlockingIfIsolating"
        ]
      }
    ],
    "compounds": []
  },
